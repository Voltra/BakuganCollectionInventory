{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "awcodes/filament-curator": "^3.6.9",
        "awcodes/filament-quick-create": "^3.4.2",
        "bezhansalleh/filament-language-switch": "^3.1",
        "filament/filament": "^3.2.115",
        "jeffgreco13/filament-breezy": "^2.4.5",
        "joaopaulolndev/filament-world-clock": "^1.0.9",
        "laravel/framework": "^11.26",
        "laravel/tinker": "^2.10",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.3.2",
        "njxqlus/filament-progressbar": "^1.0.1",
        "shuvroroy/filament-spatie-laravel-backup": "^2.2",
        "voltra/filament-svg-avatar": "^1.2.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14.3",
        "barryvdh/laravel-ide-helper": "^3.1",
        "driftingly/rector-laravel": "^1.2.4",
        "fakerphp/faker": "^1.23.1",
        "larastan/larastan": "^2.9.8",
        "laravel/pint": "^1.18.1",
        "laravel/sail": "^1.34",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.4",
        "pestphp/pest": "^3.2.5",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^1.12.5",
        "phpstan/phpstan-deprecation-rules": "^1.2.1",
        "phpstan/phpstan-phpunit": "^1.4",
        "rector/rector": "^1.2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "app:meta": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -M"
        ],
        "app:optimize": [
            "@php artisan optimize:clear",
            "@php artisan icons:cache",
            "@php artisan filament:optimize",
            "@php artisan optimize"
        ],
        "app:patch": [
            "@composer patches-relock",
            "@composer patches-repatch"
        ],
        "app:backup": [
            "@php artisan backup:run"
        ],
        "analyse": "@php ./vendor/bin/phpstan analyse",
        "format": "@php ./vendor/bin/pint",
        "refactor": [
            "@php ./vendor/bin/rector process",
            "@composer format"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi --seed",
            "@php artisan storage:link"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar",
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
